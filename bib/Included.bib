  author = {Ehlers, Anke and Grey, Nick and Wild, Jennifer and Stott, Richard and Liness, Sheena and Deale, Alicia and Handley, Rachel and Albert, Idit and Cullen, Deborah and Hackmann, Ann and Manley, John and McManus, Freda and Brady, Francesca and Salkovskis, Paul and Clark, David M.},
  year = {2013},
