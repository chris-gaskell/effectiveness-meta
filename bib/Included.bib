@article{Samstag2019,
  title = {Characteristics of Trainees' Early Sessions: {{A}} Naturalistic Process-Outcome Study Tribute to {{Jeremy D}}. {{Safran}}.},
  shorttitle = {Characteristics of Trainees' Early Sessions},
