  keywords = {Adolescent,Adult,Aged,Anxiety Disorders,Benchmarking,Clinical Competence,Cognitive Behavioral Therapy,Depression,Female,Humans,Male,Middle Aged,Patient Dropouts,Patient Satisfaction,Psychiatric Status Rating Scales,Psychology,Quality of Life,Self Report,Students; Health Occupations},
