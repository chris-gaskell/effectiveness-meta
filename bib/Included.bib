@article{Bitran2008,
  title = {A Pilot Study of Sensation-Focused Intensive Treatment for Panic Disorder with Moderate to Severe Agoraphobia: Preliminary Outcome and Benchmarking Data.},
  author = {Bitran, S and SB, Morissette and DA, Spiegel and DH, Barlow},
