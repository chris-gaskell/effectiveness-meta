@article{Chase2015,
  title = {Investigating Treatment Outcomes across {{OCD}} Symptom Dimensions in a Clinical Sample of {{OCD}} Patients},
  author = {Chase, Tannah and Wetterneck, Chad T. and Bartsch, Robert A. and Leonard, Rachel C. and Riemann, Bradley C.},
