@article{Evans2017,
  title = {The Effect of Length, Duration, and Intensity of Psychological Therapy on {{CORE}} Global Distress Scores},
  author = {Evans, Lauren Jayne and Beck, Alison and Burdett, Mark},
  year = {2017},
  month = sep,
