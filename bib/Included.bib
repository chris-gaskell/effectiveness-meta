@article{Bados2007,
  title = {Outcome of Cognitive-Behavioural Therapy in Training Practice with Anxiety Disorder Patients},
  author = {Bados, Arturo and Balaguer, Gemma and Salda{\~n}a, Carmina},
  year = {2007},
  month = nov,
  volume = {46},
  pages = {429--435},
  issn = {01446657},
  doi = {10.1348/014466507X209961},
  abstract = {Objectives. This study aims to assess the effectiveness of cognitive\textendash behavioural therapy applied by trainee therapists in patients with anxiety disorders seen in a private university service and to examine whether this effectiveness is comparable to that observed in controlled studies. Design. We compared the effectiveness of cognitive\textendash behavioural therapy at a private centre with that observed in meta-analyses and reviews of controlled studies. Methods. Out of the 96 initial patients with anxiety disorder, 64 completed the cognitive\textendash behavioural treatment and 59 also answered a series of questionnaires preand post-treatment. The effect size of the intervention was calculated, as were the percentages of patients who improved and those who recovered. Results. The pre-post effect size (1.09) was large, although somewhat lower than those observed in a meta-analysis of Spanish studies and in a range of international meta-analyses; moreover, twice as many hours of treatment were administered (M {$\frac{1}{4}$} 27:4) and the drop-out rate was higher (33.3\%). The percentages of patients who improved (61\%) and those who recovered (52.5\%) were roughly comparable to those reported in various reviews of studies performed in anxiety disorders. Conclusions. Cognitive\textendash behavioural therapy applied by trainee therapists in a private university service appears effective, although this efficacy may be somewhat lower than that in controlled studies. Moreover, the duration of treatment and the drop-out rate may be higher.},
  file = {/Users/chris/Zotero/storage/C6BDZP3L/Bados et al. - 2007 - Outcome of cognitive-behavioural therapy in traini.pdf},
  journal = {British Journal of Clinical Psychology},
  language = {en},
  number = {4}
}

