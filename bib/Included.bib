@article{Hitt2018,
  title = {The Clinical Effectiveness of a Cognitive Behavioural Therapy Intervention in a Work Setting: A 5-Year Retrospective Analysis of Outcomes.},
  author = {Hitt, David and Tahir, Tayyeb and Davies, Lyndon and Delahay, Jo and Kelson, Mark},
