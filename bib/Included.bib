  keywords = {*Mental Disorders/ec [Economics],*Mental Disorders/th [Therapy],*Outcome Assessment; Health Care/ec [Economics],*Outcome Assessment; Health Care/sn [Statistics \& Numerical Data],*Private Practice/ec [Economics],*Program Evaluation/ec [Economics],*Psychotherapy/ec [Economics],Adult,Canada,Cost-Benefit Analysis,Cost-Benefit Analysis/ec [Economics],Female,Humans,Male,Mental Disorders/px [Psychology],Psychiatric Status Rating Scales,Psychotherapy,Self Disclosure,Time Factors},
  number = {2}
}

