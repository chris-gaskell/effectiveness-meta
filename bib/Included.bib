  keywords = {Adolescence,Adult,Benchmarking,Chi Square Test,Cochrane Library,Cognition,Cognitive Therapy,Comorbidity,Comparative Studies,Confidence Intervals,Descriptive Statistics,Embase,Female,Human,Interview Guides,Interviews,Male,Medline,Middle Age,National Health Programs,Obsessive-Compulsive Disorder,Outpatients,Prospective Studies,Psychosocial Fact,Psycinfo,Randomized Controlled Trials,Scales,Standards,Systematic Review,Therapy,Treatment Outcomes,United Kingdom},
