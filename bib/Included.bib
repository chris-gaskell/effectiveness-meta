@article{Sembill2019a,
  title = {The Phase Model of Psychotherapy Outcome: {{Domain}}-Specific Trajectories of Change in Outpatient Treatment},
  shorttitle = {The Phase Model of Psychotherapy Outcome},
  author = {Sembill, Anja and Vocks, Silja and Kosfelder, Joachim and Sch{\"o}ttke, Henning},
