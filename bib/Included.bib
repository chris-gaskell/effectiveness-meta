}

@article{Kellett2013,
  title = {Cognitive Analytic Therapy for Borderline Personality Disorder: {{Therapist}} Competence and Therapeutic Effectiveness in Routine Practice.},
  author = {Kellett, Stephen and Bennett, Dawn and Ryle, Tony and Thake, Anna},
  year = {2013},
  volume = {20},
