@article{Turner2020,
  title = {Efficacy and Moderators of Cognitive Behavioural Therapy for Psychosis versus Other Psychological Interventions: An Individual-Participant Data Meta-Analysis},
  shorttitle = {Efficacy and Moderators of Cognitive Behavioural Therapy for Psychosis versus Other Psychological Interventions},
  author = {Turner, David T. and Reijnders, Mirjam and {van der Gaag}, Mark and Karyotaki, Eirini and Valmaggia, Lucia R. and Moritz, Steffen and Lecomte, Tania and Turkington, Douglas and Penad{\'e}s, Rafael and Elkis, Helio and Cather, Corinne and Shawyer, Frances and O'Connor, Kieron and Li, Zhan-Jiang and {de Paiva Barretto}, Eliza Martha and Cuijpers, Pim},
